  pod 'SnapKit', '~> 0.19'
  pod 'Spring', :git => 'https://github.com/MengTo/Spring.git', :branch => 'swift2'
  
  # External analytic services
  pod 'Crashlytics', '~> 3.6'
  pod 'Fabric', '~> 1.6'
  pod 'FBSDKCoreKit', '~> 4.10'
  pod 'GA-SDK-IOS', '~> 2.1'
  pod 'Google/Analytics'
end

target 'ELColorGameTests' do
  pod 'Google/Analytics'
  pod 'Nimble', '~> 4.0'
  pod 'Quick', '~> 0.9'
end
